{
  "name": "@vatel/sdk",
  "version": "0.1.0",
  "description": "JavaScript SDK for Call Agent Builder WebSocket and REST APIs",
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
